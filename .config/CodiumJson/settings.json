
{
  "workbench.colorTheme": "Starfall Ocean",
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "workbench.startupEditor": "none",
  "workbench.sideBar.location": "right",
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "editor.renderControlCharacters": false,
  "vim.camelCaseMotion.enable": true,
  "vim.cursorStylePerMode.insert": "line-thin",
  "vim.cursorStylePerMode.normal": "block",
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.sneak": true,
  "editor.fontFamily": "JetBrains Mono",
  "terminal.integrated.fontFamily": "FiraCode NFM",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "vim.leader": "<Space>",
  "workbench.layoutControl.enabled": false,
  "workbench.activityBar.visible": false,
  "workbench.tips.enabled": false,
  "workbench.editor.wrapTabs": true,
  "editor.cursorBlinking": "phase",
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(250, 240, 170, 0.2)",
  "vim.highlightedyank.textColor": "rgb(250, 240, 170)",
  "editor.linkedEditing": true,
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "warmUp.changeCount": "60",
  "warmUp.switchNaturalLanguage": "english",
  "warmUp.switchProgrammingLanguage": "javascript",
  "vim.easymotionDimColor": "#cccccc",
  "vim.easymotionMarkerForegroundColorOneChar": "#b2960d",
  "vim.easymotionMarkerBackgroundColor": "#00000000",
  "vim.easymotionIncSearchForegroundColor": "#b2960d",
  "vim.easymotionMarkerForegroundColorTwoCharFirst": "#b2960d",
  "window.title": "${dirty}${activeFolderShort}${separator}${activeEditorShort}${separator}${rootName}${separator}${appName}",
  "editor.fontWeight": "Regular",
  "vim.smartRelativeLine": true,
  "warmUp.changeTypingMode": "words (fixed amount)",
  "php.validate.executablePath": "C:/xampp/php/php.exe",
  "window.zoomLevel": 1,
  "css.completion.completePropertyWithSemicolon": false,
  "editor.formatOnPaste": true,
  "html.format.contentUnformatted": "",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "vim.useSystemClipboard": true,
  "prettier.jsxSingleQuote": true,
  "prettier.singleAttributePerLine": true,
  "prettier.singleQuote": true
}
