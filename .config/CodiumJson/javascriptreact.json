  "useState Snippet": {
    "prefix": "state",
    "body": ["const [$1, set${1/(.*)/${1:/capitalize}/}] = useState($0);"],
    "description": "add a useState hook"
  },
  "funtional Component": {
    "prefix": "rfc",
    "body": [
      "import React from 'react';",
      "import styles from './$1.module.css';",
      " ",
      "export default function $1(){",
      "return <>$0</>",
      "}"
    ]
  }
}
